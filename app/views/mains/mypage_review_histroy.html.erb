<%= render "shared/header"%>
<main class="l-container clearfix">
  <div class="l-content">
    <section class="mypage-tab-container">
      <h2 class="mypage-tab-head">評価一覧</h2>
      <ul class="review-history-tabs">
        <li class="active">
          <h3>
            <a href="#mypage-tab-transaction-now" data-toggle="tab">
              すべて
            </a>
          </h3>
        </li>
        <li class="">
          <h3>
            <a href="/mypage/review/history/great">
              <div>
                <i class="icon-good">
                </i>
                 良い
              </div>
            </a>
          </h3>
        </li>
        <li class="">
          <h3>
            <a href="/mypage/review/history/good">
              <div>
                <i class="icon-normal">
                </i>
                 普通
              </div>
            </a>
          </h3>
        </li>
        <li class="">
          <h3>
            <a href="/mypage/review/history/poor">
              <div>
                <i class="icon-bad">
                </i>
                 悪い
              </div>
            </a>
          </h3>
        </li>
      </ul>
      <div class="tab-content">
        <ul class="mypage-item-list">
          <li class="mypage-item-not-found bold">
            評価はまだありません
          </li>
        </ul>
      </div>
    </section>
    <ul class="mypage-history pager">
      <li class="pager-prev pager-cell">
        <a href="/mypage/review/history/">
          <i class="icon-arrow-left">
          </i>
        </a>
      </li>
      <li class="pager-next pager-cell">
        <a href="/mypage/review/history/">
          <i class="icon-arrow-right">
          </i>
        </a>
      </li>
    </ul>
  </div>
  <%= render "shared/sidebar" %>
</main>

<%= render "shared/seller-button" %>
<%= render "shared/footer" %>
